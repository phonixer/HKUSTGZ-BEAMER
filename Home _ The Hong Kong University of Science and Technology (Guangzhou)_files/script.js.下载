var remove_table_row_style = function() {
  console.log('removing td width');
  $('div[class="hkust-simple-text"] table tr td').each(function(i, e) {
    if (e.style) {
      e.style.width = "";
    }
  });
}


window.onload = function() {
  var $ = jQuery;
  
  //People Auto Tabbed with URL Query "tab"
  var urlSearchParams = new URLSearchParams(window.location.search);
  var params = Object.fromEntries(urlSearchParams.entries());
 
  if(params.tab != "") {  
      $('.tab').removeClass('active');
      $('.tab-heading').removeClass('active');
      $('.tab[data-tab-id=' + params.tab + ']').addClass('active');
      $('.tab-content').css('height', 0);
      $('.tab[data-tab-id=' + params.tab + '] .tab-content').css('height','auto');
      $('.tab-heading[data-tab-id=' + params.tab + ']').addClass('active');
  }
  
  if ($('.slider')) {
	  $('.slider').slick({
	  infinite: true,
	  slidesToShow: 4, 
	  slidesToScroll: 4, 
	  autoplay: true,
	  autoplaySpeed: 4000,
	       
	  responsive: [
	  {
	    breakpoint: 1280,
	    settings: {
	      slidesToShow: 3,
	      slidesToScroll: 3
	    }
	  },
	  {
	    breakpoint: 992,
	    settings: {
	      slidesToShow: 2,
	      slidesToScroll: 2
	    }
	  }, 
	  {
	    breakpoint: 640,
	    settings: {
	      slidesToShow: 1,
	      slidesToScroll: 1
	    }
	  }
	]});
	}
	
	// video silder
	if($('#video-silder')) {
