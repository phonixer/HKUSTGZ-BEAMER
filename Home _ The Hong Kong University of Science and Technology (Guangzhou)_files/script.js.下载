  ;(function countdownSchoolOpen() {
    const countdownTimeWrap = document.querySelector('#countdown-time-wrap')
    if (!countdownTimeWrap) { return }
    const today = +new Date()
    const schoolOpenDay = +new Date('2022-09-01')
    const dayRemaining = Math.floor((schoolOpenDay - today) / (1000 * 24 * 60 * 60))
    const percentile = Math.floor(dayRemaining / 100)
    const tenths = Math.floor(dayRemaining / 10) % 10
    const quantile = dayRemaining % 10
    let str = ''
    percentile > 0 && (str += `
    <span class="countdown-time__item">${percentile}</span>`)
    if (tenths > 0 || percentile > 0 && tenths === 0) {
      str += `
      <span class="countdown-time__item">${tenths}</span>`
    }
    if (!(percentile <= 0 && tenths <= 0 && quantile < 0 )) {
      str += `
      <span class="countdown-time__item">${quantile}</span>`
    }
    countdownTimeWrap.innerHTML = str
  })()
  
  
  remove_table_row_style();
  
  
  let nodeList = document.querySelectorAll('.slick-slide');
  let observeList = Array.apply(null, nodeList)
  // let showTargetsList = observeList.slice(-2)
  let showTargetsList = []
  let countDown = $('div[id="countdownSchoolOpen"]').parent();
  let config_other = { attributes: true };
  
  let importantNotice = $('#home-important-notice')
  
  const hiddenTargets = [countDown, importantNotice]
  
  
  for (let i = 0 ; i < observeList.length; i++) {
    let item = observeList[i];
    let callback_other = function(mutationsList, observer) {
      for(let mutation of mutationsList) {
        if (mutation.type === 'attributes' && mutation.attributeName === 'style') {
          
          for(let hideTarget of hiddenTargets) {
            if (!hideTarget || hideTarget.length === 0) {
              continue
            }
            if (showTargetsList.some(d => d === item)) {
              if (item.style.opacity !== 0) {
                hideTarget[0].style.visibility = 'hidden';
              } else {
                hideTarget[0].style.visibility = 'visible';
              }
            } else {
              hideTarget[0].style.visibility = 'visible';
            }
          }
        }
      };
    }
  
    new MutationObserver(callback_other).observe(item, config_other)
  }
  
  var statement = document.querySelector('#statement')
  var statement_close_btn = document.querySelector('#statment_close_btn')

  statement_close_btn && statement_close_btn.addEventListener('click', function () {
  statement.style.display = 'none'
  })

  
  var application_tip = document.querySelector('#application_tip')
  var application_tip_close = document.querySelector('#apply_close_btn')
  
  var apply_now = document.querySelector('.apply-entrance-tips .hkust-single-image')
  
  if (application_tip) {
    application_tip.style.display = 'none'
  }
  function startCountdown() {
    if (!application_tip) return false
    application_tip.style.display = 'block'
  }
  
  apply_now && apply_now.addEventListener('click', startCountdown)
  let goto_apply = document.querySelector('#goto_apply')
  goto_apply && goto_apply.addEventListener('click', startCountdown)
  application_tip_close && application_tip_close.addEventListener('click', function () {
    application_tip.style.display = 'none'
  })
};

